// 上传图片请求消息
message UploadImageRequest {
  bytes imageData = 1;  // 图片的二进制数据
  string fileName = 2;  // 图片文件的名称
  string project = 3;   // 项目名称
}

// 上传图片响应消息
message UploadImageResponse {
  string imageName = 1;   // 图片名称
}

// 获取图片 URL 请求消息
message GetImageUrlRequest {
  string imageName = 1;  // 要获取的图片的唯一标识符
  string imageUrl = 2;
}

// 获取图片 URL 响应消息
message GetImageUrlResponse {
  string imageUrl = 1;  // 图片的 URL
}
// ImageService 提供图片上传和获取 URL 的功能
service ImageService {
  // 上传图片
  rpc UploadImage(UploadImageRequest) returns (UploadImageResponse);
  // 获取图片 URL
  rpc GetImageUrl(GetImageUrlRequest) returns (GetImageUrlResponse);
}
