message SearchSysUserResp {
  repeated SysUser data = 1; //sysUser
  int64 total = 2; //total
}
message ResetPasswordReq {
  uint64 id = 1; //id
}
message ResetPasswordResp {
}

message FrozenAccountsReq {
  uint64 id = 1; //id
  int64 isDisabled = 2;
}
message FrozenAccountsResp {
}

message RecoverAccountsReq {
  uint64 id = 1; //id
}
message RecoverAccountsResp {
}

message ChangePasswordReq {
  uint64 id = 1; //id
  string oldPassword = 2; //旧密码
  string newPassword = 3; //新密码
  string confirmNewPassword = 4;
}

message ChangePasswordResp {
}

message ChangeOrganizationsReq {
  uint64 id = 1;
  int64 organizationId = 2;
}
message ChangeOrganizationsResp {
}

message ChangePositionsReq {
  uint64 id = 1;
  int64 positionId = 2;
}

message ChangePositionsResp {
}

message LoginReq {
  string account = 1; //账号
  string password = 2; //密码
}

message LoginResp {
  uint64 accountId = 1; // 登录是否成功
  string message = 2; // 返回信息
}

service SysUserService {
  //-----------------------账号信息表-----------------------
  rpc AddSysUser(AddSysUserReq) returns (AddSysUserResp);
  rpc UpdateSysUser(UpdateSysUserReq) returns (UpdateSysUserResp);
  rpc DelSysUser(DelSysUserReq) returns (DelSysUserResp);
  rpc GetSysUserById(GetSysUserByIdReq) returns (GetSysUserByIdResp);
  rpc SearchSysUser(SearchSysUserReq) returns (SearchSysUserResp);

  rpc ResetPassword(ResetPasswordReq) returns (ResetPasswordResp);
  rpc FrozenAccounts(FrozenAccountsReq) returns (FrozenAccountsResp);
  rpc RecoverAccounts(RecoverAccountsReq) returns (RecoverAccountsResp);
  rpc ChangePassword(ChangePasswordReq) returns (ChangePasswordResp);
  rpc ChangeOrganizations(ChangeOrganizationsReq) returns (ChangeOrganizationsResp);
  rpc ChangePositions(ChangePositionsReq) returns (ChangePositionsResp);
  rpc AccountLogin(LoginReq) returns (LoginResp);
}

